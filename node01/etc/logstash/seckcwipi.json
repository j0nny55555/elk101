{
    "template" : "seckcwipi*",
    "order" : 1,
    "version" : 50001,
    "settings" : {
        "index.refresh_interval" : "5s"
    },
    "mappings" : {
        "syslogwithgeo" : {
            "properties" : {
                "@timestamp" : {
                    "type" : "date",
                    "include_in_all" : false
                },
                "@version" : {
                    "type": "keyword",
                    "include_in_all" : false
                },
                "syslog_pri" : {
                    "type" : "short"
                },
                "location" : {
                    "type" : "geo_point"
                },
                "embedded_timestamp" : {
                    "type" : "date"
                },
                "syslog_hostname" : {
                    "type" : "text"
                },
                "syslog_program" : {
                    "type" : "text"
                },
                "syslog_pid" : {
                    "type" : "integer"
                },
                "json_message" : {
                    "type" : "json",
                    "enabled" : true
                },
                "source" : {
                    "type" : "text"
                },
                "status" : {
                    "type" : "text"
                },
                "messagetype" : {
                    "type" : "text"
                }
            }
        }
    }
}